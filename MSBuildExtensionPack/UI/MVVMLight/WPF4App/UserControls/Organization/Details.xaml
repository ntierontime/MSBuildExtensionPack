		<TextBlock Text="{Binding Path=NvarcharColumn, Mode=OneWay}" Grid.Column="1" Grid.Row="11" />
		<TextBlock Text="{Binding Path=MSBuildExtensionPack_Resx_UIStringResourcePerEntityOrganization.NtextColumn, Source={StaticResource LocalizedStrings}}" Grid.Column="0" Grid.Row="12" />
		<TextBlock Text="{Binding Path=NtextColumn, Mode=OneWay}" Grid.Column="1" Grid.Row="12" />
 		<CheckBox Content="{Binding Path=MSBuildExtensionPack_Resx_UIStringResourcePerEntityOrganization.BitColumn, Source={StaticResource LocalizedStrings}}" ToolTip="{Binding Path=MSBuildExtensionPack_Resx_UIStringResourcePerEntityOrganization.BitColumn, Source={StaticResource LocalizedStrings}}" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Column="1" Grid.Row="13" IsChecked="{Binding Path=BitColumn, Mode=OneWay}" IsEnabled="False" />
		<TextBlock Text="{Binding Path=MSBuildExtensionPack_Resx_UIStringResourcePerEntityOrganization.BinaryColumn, Source={StaticResource LocalizedStrings}}" Grid.Column="0" Grid.Row="14" />
		<TextBlock Text="{Binding Path=BinaryColumn, Mode=OneWay}" Grid.Column="1" Grid.Row="14" />
		<TextBlock Text="{Binding Path=MSBuildExtensionPack_Resx_UIStringResourcePerEntityOrganization.VarbinaryColumn, Source={StaticResource LocalizedStrings}}" Grid.Column="0" Grid.Row="15" />
