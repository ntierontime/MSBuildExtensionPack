<?xml version="1.0" encoding="utf-8"?>
<Application
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:vm="clr-namespace:MSBuildExtensionPack.MVVMLightViewModels;assembly=MSBuildExtensionPack.MVVMLightViewModels.X"
    xmlns:fxr="clr-namespace:Framework.Xamariner;assembly=Framework.Xamariner.X"
    x:Class="MSBuildExtensionPack.XamarinForms.App">
    <Application.Resources>
		<ResourceDictionary x:Name="appMain">
			<vm:ViewModelLocator x:Key="Locator" />
            <fxr:EnumToNameValuePairConverter x:Key="EnumToNameValuePairConverter" />
			<Style x:Key="Heading" TargetType="Label">
				<Setter Property="FontSize" Value="18" />
				<Setter Property="TextColor" Value="AntiqueWhite" />
				<Setter Property="BackgroundColor" Value="Blue" />
			</Style>
			<Style x:Key="SubHeading" TargetType="Label">
				<Setter Property="FontSize" Value="12" />
				<Setter Property="TextColor" Value="AntiqueWhite" />
				<Setter Property="BackgroundColor" Value="Blue" />
			</Style>
		</ResourceDictionary>
	</Application.Resources>
</Application>
