    <ContentPage.ToolbarItems>
        <iconize:IconToolbarItem Icon="ep-500px" />
        <iconize:IconToolbarItem Icon="ep-500px" IconColor="Red" />
        <!--<ToolbarItem Text="Add" Priority="0" Order="Secondary"  />
        <ToolbarItem Text="Cancel" Priority="1" Order="Secondary"  />-->
    </ContentPage.ToolbarItems>
</MasterDetailPage>
